{
  "title": "Read More Article Inline",
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "coco/read-more-inline",
  "version": "1.0",
  "description": "Insert a Related Article Inline (Read Next aligned to left) with the paragraphs, on the left side of the content.",
  "category": "widgets",
	"icon": "post-status",
  "supports": {
    "renaming": false
  },
  "attributes": {
    "source": {
      "type": "string",
      "default": "category"
    },
    "termID": {
      "type": "number",
      "default": 0
    },
    "postID": {
      "type": "number",
      "default": 0
    }
  },
  "example": {
    "attributes": {
      "source": "category",
      "termID": 1
    }
  },
	"editorScript": "file:./index.js",
	"render": "file:./render.php",
	"style": "file:./style-index.css"
}
